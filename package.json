{
	"name": "pdf-summary-generator",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"engines": {
		"node": ">=20.*"
	},
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:csr": "ng serve --no-hmr --port=3000",
		"start:hmr": "ng serve --port=3000",
		"start:dev-ssr": "nodemon --config=nodemon.json",
		"start:prod-ssr": "node dist/pdf-summary-generator/server/server.mjs",
		"build:dev": "ng build --watch --configuration=development",
		"build:prod": "ng build",
		"dev:ssr": "concurrently \"npm run build:dev\" \"npm run start:dev-ssr\"",
		"prod:ssr": "npm run build:prod && npm run start:prod-ssr",
		"refresh": "rm -rf .angular dist node_modules package-lock.json && ng cache clean && npm cache clean --f && npm install",
		"pre-commit": "npm run format && npm run lint:fix",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,html,css,scss}\"",
		"lint": "eslint \"{src,public}/**/*.{js,jsx,ts,tsx,html,css,scss}\" --fix",
		"lint:fix": "eslint \"{src,public}/**/*.{js,jsx,ts,tsx,html,css,scss}\" --fix --quiet --fix-type layout --format=codeframe",
		"typesync": "typesync"
	},
	"dependencies": {
		"@angular/animations": "^20.0.3",
		"@angular/cdk": "^20.0.3",
		"@angular/common": "^20.0.3",
		"@angular/compiler": "^20.0.3",
		"@angular/core": "^20.0.3",
		"@angular/forms": "^20.0.3",
		"@angular/material": "^20.0.3",
		"@angular/platform-browser": "^20.0.3",
		"@angular/platform-browser-dynamic": "^20.0.3",
		"@angular/platform-server": "^20.0.3",
		"@angular/router": "^20.0.3",
		"@angular/ssr": "^20.0.3",
		"core-js": "^3.43.0",
		"express": "^5.1.0",
		"ngx-spinner": "^19.0.0",
		"pdfjs-dist": "^5.4.54",
		"rxjs": "~7.8.2",
		"tslib": "^2.8.1",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-eslint/builder": "^20.0.0",
		"@angular-eslint/eslint-plugin": "^20.0.0",
		"@angular-eslint/eslint-plugin-template": "^20.0.0",
		"@angular-eslint/schematics": "^20.0.0",
		"@angular-eslint/template-parser": "^20.0.0",
		"@angular/build": "^20.0.2",
		"@angular/cli": "^20.0.2",
		"@angular/compiler-cli": "^20.0.3",
		"@tailwindcss/postcss": "^4.1.4",
		"@types/core-js": "~2.5.8",
		"@types/eslint": "^9.6.1",
		"@types/express": "^5.0.1",
		"@types/node": "^20.17.19",
		"@typescript-eslint/eslint-plugin": "7.18.0",
		"@typescript-eslint/parser": "7.18.0",
		"angular-eslint": "^20.0.0",
		"concurrently": "^9.2.0",
		"daisyui": "^5.0.43",
		"dotenv": "^16.5.0",
		"eslint": "8.57.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-formatter-codeframe": "^7.32.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.6",
		"nodemon": "^3.1.10",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "^4.1.4",
		"tailwindcss-animate": "^1.0.6",
		"typescript": "~5.8.3",
		"typesync": "^0.14.3"
	}
}
